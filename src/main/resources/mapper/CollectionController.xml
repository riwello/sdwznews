<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liweile.news.mapper.CollectMapper">

    <select id="getCollectList" resultType="Integer" parameterType="String">
        SELECT newsId FROM collect WHERE username =#{username}
    </select>

    <insert id="addCollect" parameterType="com.liweile.news.model.Collection">
    insert into collect (newsId, username)

    values (#{newsId}, #{username})

    </insert>

</mapper>